{
  "extends": "../tsconfig.base.json",
  "compilerOptions": {
    "paths": { "@angular/*": ["./node_modules/@angular/*"] },
    "outDir": "../out-tsc/app",
    "baseUrl": "./"
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "../node_modules/@nebular/**/*.spec.ts"
  ],
  "include": [
    "../src/*.ts",
    "../src/**/*.ts",
    "../node_modules/@nebular/**/*.ts"
  ]
}
